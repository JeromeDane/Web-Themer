{
	"manifest_version": 2,
	"name": "__MSG_extName__",
	"description": "__MSG_extDesc__",
	"version": "0.0.1",
	"permissions": [
		"bookmarks",
		"tabs"
	],
	"default_locale": "en",
	"options_page": "options.html",
	"content_scripts": [ {
      "all_frames": true,
      "js": [ 
      	"com/jeromedane/webthemer/Themer.js", 
      	"content.js" 
      ],
      "matches": [ 
      	"http://*/*", 
      	"https://*/*" 
      ],
      "run_at": "document_start"
   	}],
	"browser_action": {
		"default_icon": "icon_16.png"
	},
	"background": {
		"scripts": ["com/jeromedane/webthemer/Themer.js", "background.js"],
		"persistent": true
	}
   
}